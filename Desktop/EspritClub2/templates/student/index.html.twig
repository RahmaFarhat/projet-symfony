<h1>
  Student index
</h1>
{{ dump(students) }}
<table border="2">
  <thead>
    <tr>
      <th>
        Id
      </th>
      <th>
        Nom
      </th>
      <th>
        Age
      </th>
      <th>
        actions
      </th>
    </tr>
  </thead>
  <tbody>
    {% for student in students %}
      <tr>
        <td>
          {{ student.id }}
        </td>
        <td>
          {{ student.nom }}
        </td>
        <td>
          {{ student.age }}
        </td>
        <td>
          {# <a href="{{
              path(
                'app_classroom_read',
                {
                  id: classroom.id
                }
              )
              }}">
              show
            </a> #}
          {# <a href="{{
              path(
                'app_classroom_edit',
                {
                  id: classroom.id
                }
              )
              }}">
              edit
            </a> #}
          <a href="{{
            path(
              'app_student_delete',
              {
                id: student.id
              }
            )
            }}">
            delete
          </a>
        </td>
      </tr>
    {% else %}
      <tr>
        <td colspan="3">
          no records found
        </td>
      </tr>
    {% endfor %}
  </tbody>
</table>

